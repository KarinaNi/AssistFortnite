{
  "expo": {
    "name": "Assistant for Fortnite",
    "slug": "fortniteAssistant",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "icon": "./assets/images/icon.png",
    "splash": {
      "backgroundColor": "#4E4C4C",
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "notification": {
      "icon": "./assets/images/notificon.png",
      "color": "#9A35C9",
      "androidMode": "default"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.cs560.AssistantForFortnite"
    },
    "description": "",
    "githubUrl": "https://github.com/KarinaNi/AssistFortnite"
  }
}